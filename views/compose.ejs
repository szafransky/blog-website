<%- include('partials/header') -%>
<h1>Compose</h1>
<form class="" action="/add-post" method="post">
  <div class="mb-3">
    <label for="title" class="form-label">Title</label>
    <input type="text" class="form-control" id="title" name='postTitle'>
  </div>
  <div class="mb-3">
    <label for="post" class="form-label">Post</label>
    <textarea class="form-control" name="postBody" rows="8" cols="80" id='post'></textarea>
  </div>

  <button class='btn btn-primary mt-2' type="post" name="button">Publish</button>
</form>


<%- include('partials/footer') -%>
